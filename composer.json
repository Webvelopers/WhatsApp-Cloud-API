{
    "name": "webvelopers/whatsapp-cloud-api",
    "authors": [{
        "name": "Webvelopers, Inc.",
        "email": "webmaster@webvelopers.net"
    }],
    "type": "library",
    "description": "WhatsApp Cloud API is a free library, this package makes it easy for developers to access WhatsApp Cloud API service in their Laravel projects.",
    "keywords": [
        "library",
        "laravel",
        "WhatsApp",
        "WhatsApp Cloud API",
        "Facebook",
        "META"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "vlucas/phpdotenv": "^5.4",
        "myclabs/php-enum": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.19"
    },
    "autoload": {
        "psr-4": {
            "Webvelopers\\WhatsAppCloudAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webvelopers\\WhatsAppCloudAPI\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
